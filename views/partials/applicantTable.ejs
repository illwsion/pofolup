<table class="tg">
  <thead>
    <tr>
      <th class="tg-lboi">번호</th>
      <th class="tg-cly1">이름</th>
      <th class="tg-cly1">이메일</th>
      <th class="tg-cly1">지원분야</th>
      <th class="tg-cly1">대표 이미지</th>
      <th class="tg-cly1">지원 경로</th>
      <th class="tg-cly1">최근 변경 날짜</th>
      <th class="tg-cly1">삭제 버튼</th>
    </tr>
  </thead>
  <tbody>
    <% for(var i=0; i<Applicants.length; i++) { %>
    <tr>
      <td class="tg-cly1"><%= Applicants.length-i %></td>
      <td class="tg-cly1"> <%= Applicants[i].realname%> </td>
      <td class="tg-cly1"> <a href="/applicants/<%= Applicants[i].username%>"><%= Applicants[i].username%></a></td>
      <td class="tg-cly1"><%= Applicants[i].position%></td>
      <td class="tg-cly1"><%= Applicants[i].file%><img src="https://wonyframe-recruit-website.s3.ap-northeast-2.amazonaws.com/image/<%= Applicants[i].file%>" width = "50px", height ="50px") alt="">  </td>
      <td class="tg-cly1"><%= Applicants[i].route%></td>
      <td class="tg-cly1"><%= Applicants[i].updateDate%></td>
      <td class="tg-cly1"> <a href="/deleteApplicant/<%= Applicants[i]._id %>">지원자 삭제</a></td>
    </tr>
    <%}%>
  </tbody>
</table>
