<table class="tg" style="width:100%">
  <thead>
    <tr>
      <th class="tg-cly1">이름</th>
      <th class="tg-cly1">이메일</th>
      <th class="tg-cly1">지원분야</th>
      <th class="tg-cly1">대표 이미지</th>
      <th class="tg-cly1">지원 경로</th>
      <th class="tg-cly1">태그</th>
      <th class="tg-cly1">삭제 버튼</th>
    </tr>
  </thead>
  <tbody>
    <% for(var i=0; i<Applicants.length; i++) { %>
    <tr>
      <td class="tg-cly1" style="width:10%"> <%= Applicants[i].realname%>
          <% if (Applicants[i].isAdmin) { %>
            <p>관리자</p>
          <% } %>
      </td>
      <td class="tg-cly1" style="width:10%"> <a href="/applicants/<%= Applicants[i].username%>"><%= Applicants[i].username%></a></td>
      <td class="tg-cly1" style="width:10%"><%= Applicants[i].position%></td>
      <td class="tg-cly1" style="width:20%"><%= Applicants[i].file%><img src="https://pofolup.s3.ap-northeast-2.amazonaws.com/image/<%= Applicants[i].username%>/<%= Applicants[i].file%>" width="50px" , height="50px" ) alt=""> </td>
      <td class="tg-cly1" style="width:10%"><%= Applicants[i].route%></td>
      <td class="tg-cly1" style="width:20%">
        <% for (let j=0; j<Applicants[i].userTags.length; j++) { %>
          <span><%= Applicants[i].userTags[j] %></span><br>
        <% } %>
      </td>
      <td class="tg-cly1" style="width:10%"> <a href="/deleteApplicant/<%= Applicants[i]._id %>">지원자 삭제</a></td>
    </tr>
    <%}%>
  </tbody>
</table>
